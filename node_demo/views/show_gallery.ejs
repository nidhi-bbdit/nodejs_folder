<%- include('header') -%>
<!DOCTYPE html>
<html lang="en">
    <head>
        
    <title></title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>
    <body>
        <h3>Image Gallery</h3>
       
        <div class="tz-gallery">
            <div class="row">
                 <%  if(result){
                    result.forEach(function(el, index){ %> 
                <div class="col-sm-12 col-md-4">
                    <img src="uploads/<%=el.image%>" alt="" height="50%" width="50%">
                </div>
                <% })
                }
                %> 
            </div> 
        </div>
    </body>
</html>