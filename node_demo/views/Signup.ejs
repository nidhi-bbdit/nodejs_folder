<%- include('header') -%>
<!DOCTYPE html>
<html lang="en">
<head>
    
  <title></title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <script src="/registration.js"></script>
  
</head>
<body>
   
    <h3><%=title %></h3>
    <form id="reg_frm" method="POST" enctype="multipart/form-data">
        <span id="error_msg" style="color:red;"></span>
        <div class="form-group row">
            <label for="staticName" class="col-sm-2 col-form-label">Name</label>
            <div class="col-sm-10" id="name_div">
                <input type="text"  class="form-control" id="staticName" value="" name="uname">
            </div>
        </div>
        <div class="form-group row">
            <label for="staticEmail" class="col-sm-2 col-form-label">Email</label>
            <div class="col-sm-10" id="email_div">
            <input type="text" class="form-control" id="staticEmail" value="" name="uemail">
            </div>
        </div>
        <div class="form-group row">
            <label for="inputPassword" class="col-sm-2 col-form-label">Password</label>
            <div class="col-sm-10" id="pwd_div">
            <input type="password" class="form-control" id="inputPassword" placeholder="Password" name="upassword">
            </div>
        </div>
        <div class="form-group row">
            <label for="staticPhone" class="col-sm-2 col-form-label">Phone No</label>
                <div class="col-sm-10" id="phno_div">
                <input type="text"  class="form-control" id="staticPhone" value="" name="phno">
            </div>
        </div>
        <div class="form-group row">
            <label for="staticPhone" class="col-sm-2 col-form-label">Profile Image</label>
                <div class="col-sm-10" id="profile_img">
                <input type="file"  class="form-control" id="staticImg" value="" name="profile_img">
            </div>
        </div>
        <div class="form-group row">
            <input type="button" class="btn btn-primary" value="Submit" id="regbtn">
        </div>
    </form>

   
</body>
</html>

